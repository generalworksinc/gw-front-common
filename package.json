{
	"name": "@generalworks/gw-front-common",
	"version": "0.0.7",
	"description": "Common frontend utilities and UI components for generalworks projects",
	"type": "module",
	"exports": {
		".": "./dist/core.js",
		"./core": "./dist/core.js",
		"./solid": "./dist/solid/mod.js",
		"./solid/components": "./dist/solid/components.js",
		"./vue": "./dist/vue/mod.js",
		"./vue/components": "./dist/vue/components.js"
	},
	"scripts": {
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"jsr:check": "bunx --yes jsr publish --dry-run",
