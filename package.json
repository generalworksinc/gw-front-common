		"vue": ">=3.4.0",
		"dayjs": ">=1.11.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.0.0",
		"@types/clone": "^2.1.4",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitejs/plugin-vue-jsx": "^4.0.0",
		"dayjs": "^1.11.0",
		"solid-js": "^1.9.0",
		"vite": "^7.1.3",
		"vite-plugin-dts": "^4.5.4",
		"vue": "^3.4.0"
