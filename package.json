		"lint:fix": "biome check --write .",
		"lint:fix-unsafe": "biome check --write --unsafe .",
		"format": "biome format --write .",
		"build": "vite build"
	},
	"dependencies": {
		"clone": "^2.1.2",
		"ofetch": "^1.4.0"
	},
	"peerDependencies": {
