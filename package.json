{
  "name": "@generalworks/gw-front-common",
  "version": "0.0.55",
  "description": "Common frontend utilities and UI components for generalworks projects",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/core/mod.d.ts",
      "default": "./dist/core/mod.js"
    },
    "./core": {
      "types": "./dist/core/mod.d.ts",
      "default": "./dist/core/mod.js"
    },
    "./solid": {
      "types": "./dist/solid/mod/index.d.ts",
      "solid": {
        "development": "./dist/solid/mod/server.js",
        "browser": "./dist/solid/mod/index.js",
        "import": "./dist/solid/mod/server.js",
        "node": "./dist/solid/mod/server.js"
      },
      "browser": "./dist/solid/mod/index.js",
      "node": "./dist/solid/mod/server.js",
      "import": "./dist/solid/mod/server.js",
      "default": "./dist/solid/mod/server.js"
    },
    "./solid/components": {
      "types": "./dist/solid/components/index.d.ts",
      "solid": {
        "development": "./dist/solid/components/server.js",
        "browser": "./dist/solid/components/index.js",
        "import": "./dist/solid/components/server.js",
        "node": "./dist/solid/components/server.js"
      },
      "browser": "./dist/solid/components/index.js",
      "node": "./dist/solid/components/server.js",
      "import": "./dist/solid/components/server.js",
      "default": "./dist/solid/components/server.js"
    },
    "./vue": {
      "types": "./dist/vue/mod.d.ts",
      "default": "./dist/vue/mod.js"
    },
    "./vue/components": {
      "./solid": {
        "types": "./dist/solid/mod/index.d.ts",
        "solid": {
          "development": "./dist/solid/mod/dev.js",
          "import": "./dist/solid/mod/index.js"
        },
        "node": "./dist/solid/mod/server.js",
        "import": "./dist/solid/mod/index.js",
        "default": "./dist/solid/mod/index.js"
      },
      "./solid/components": {
        "types": "./dist/solid/components/index.d.ts",
        "solid": {
          "development": "./dist/solid/components/server.js",
          "import": "./dist/solid/components/index.js"
        },
        "node": "./dist/solid/components/server.js",
        "import": "./dist/solid/components/index.js",
        "default": "./dist/solid/components/index.js"
      },
      "types": "./dist/vue/components.d.ts",
      "default": "./dist/vue/components.js"
    },
    "./vue/nuxt/module": {
      "types": "./dist/vue/nuxt/module.d.ts",
      "default": "./dist/vue/nuxt/module.js"
    }
  },
  "scripts": {
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "jsr:check": "bunx --yes jsr publish --dry-run --allow-slow-types --allow-dirty",
    "lint": "biome check core vue solid __tests__ ./*.ts",
    "lint:fix": "biome check --write core vue solid __tests__ ./*.ts",
    "lint:fix-unsafe": "biome check --write --unsafe core vue solid __tests__ ./*.ts",
    "format": "biome format --write core vue solid __tests__ ./*.ts",
    "build": "tsup && vite build",
    "typecheck": "tsc -p ./tsconfig.build.json --noEmit",
    "fix": "bun run lint:fix && bun run format",
    "check": "bun run lint && bun run typecheck"
  },
  "dependencies": {
    "clone": "^2.1.2"
  },
  "peerDependencies": {
    "@nuxt/kit": ">=3.0.0",
    "solid-js": ">=1.9.0",
    "vue": ">=3.4.0",
    "dayjs": ">=1.11.0",
    "pinia": ">=2.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@nuxt/kit": "^3.19.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@solid-primitives/storage": "^4.3.3",
    "@types/clone": "^2.1.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "dayjs": "^1.11.18",
    "pinia": "^2.3.1",
    "solid-js": "^1.9.9",
    "tsup": "^8.5.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "~5.6.3",
    "vite": "^7.1.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-solid": "^2.11.8",
    "vue": "^3.5.21"
  }
}
